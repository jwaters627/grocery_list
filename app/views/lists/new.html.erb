

<%= link_to("<< Back to Lists", lists_path, :class => 'back_link') %>


<div class='lists new'>
	<h2>Create new List</h2>

	<%= form_for(@list, :url => lists_path, :method => 'post') do |f| %>

		<table summary="Lists form fields">
			<tr>
				<th>Name</th>
				<td><%= f.text_field(:name) %></td>
			</tr>
		</table>

		<div class="form-buttons">
			<%= f.submit("Create List") %>
		</div>
	<% end %>

</div>

